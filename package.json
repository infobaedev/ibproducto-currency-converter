{
  "name": "ib-producto-currency-converter",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-number-format": "^4.3.1",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "aws s3 sync build/ s3://widget-conversor.economico.infobae.com --acl public-read",
    "sass": "node-sass -w ./scss -o ./src/css/",
    "c": "node-sass ./scss -o .|postcss ./src/css/ -u autoprefixer -r",
    "w": "watch 'npm run c' ./scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.2",
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.13.0",
    "postcss-cli": "^6.1.3"
  }
}
